@model SomatusAnalytics.Models.PreviewPDFConfig
@{
    Layout = "~/Views/Shared/_LayoutUser.cshtml";
}



@if (!string.IsNullOrEmpty(Model.error))
{
    <div id="errorWrapper">
        <h2>
            Error
        </h2>
        <pre>
            @Model.error
        </pre>
    </div>

    return;
}
<div class="admin content static-content-block preview" data-ref="init_pdf_preview">

    <embed src="@Model.fileURL" type="application/pdf" style="min-height:calc(100vh - 30px);width:100%" id="embed-pdf-preview"/>

</div>
